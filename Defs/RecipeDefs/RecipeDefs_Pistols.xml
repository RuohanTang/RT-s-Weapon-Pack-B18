     <RecipeDef>
    <defName>FiveSevenCraft</defName>
    <label>Make FN Five Seven Pistol</label>
    <description>Craft FN Five Seven.</description>
    <jobString>Making FN Five Seven.</jobString>
	<workSpeedStat>SmithingSpeed</workSpeedStat> 
 <effectWorking>Smith</effectWorking> 
 <soundWorking>Recipe_Machining</soundWorking> 
 <unfinishedThingDef>UnfinishedGun</unfinishedThingDef> 
    <workAmount>22000</workAmount>
	<workSkill>Crafting</workSkill>
    <products>
        <Gun_FiveSeven_rt>1</Gun_FiveSeven_rt>
    </products>
    <ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>Steel</li>
				</thingDefs>
			</filter>
			<count>45</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>Component</li>
				</thingDefs>
			</filter>
			<count>2</count>
		</li>
    </ingredients>
	<fixedIngredientFilter>
         <thingDefs>
            <li>Steel</li>
			<li>Component</li>
         </thingDefs>
    </fixedIngredientFilter>
	<recipeUsers>
		<li>RT_WeaponAssemblyTable</li>
		
	</recipeUsers>
	<researchPrerequisite>RT_Pistols</researchPrerequisite>
		<skillRequirements>
				<Crafting>3</Crafting>
		</skillRequirements>	
  </RecipeDef>


