   <RecipeDef>
    <defName>BerettaM12Craft</defName>
    <label>Make Beretta M12 SMG</label>
    <description>Craft Beretta M12.</description>
    <jobString>Making Beretta M12</jobString>
	<unfinishedThingDef>UnfinishedGun</unfinishedThingDef>
	<workSpeedStat>SmithingSpeed</workSpeedStat> 
 <effectWorking>Smith</effectWorking> 
 <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>36000</workAmount>
	<workSkill>Crafting</workSkill>
    <products>
        <Gun_BerettaM12_rt>1</Gun_BerettaM12_rt>
    </products>
    <ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>Steel</li>
				</thingDefs>
			</filter>
			<count>80</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>Component</li>
				</thingDefs>
			</filter>
			<count>2</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>WoodLog</li>
				</thingDefs>
			</filter>
			<count>1</count>
		</li>
    </ingredients>
	<fixedIngredientFilter>
         <thingDefs>
            <li>Steel</li>
			<li>Component</li>
			<li>WoodLog</li>
         </thingDefs>
    </fixedIngredientFilter>
	<recipeUsers>
		<li>RT_WeaponAssemblyTable</li>
		
	</recipeUsers>
	<researchPrerequisite>RT_Smgs</researchPrerequisite>
	<skillRequirements>
<Crafting>5</Crafting>
	</skillRequirements>
  </RecipeDef>  

